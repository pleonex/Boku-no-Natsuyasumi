    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Launch (console)",
            "type": "coreclr",
            "request": "launch",
            // "preLaunchTask": "Cake: Run Build",
            "program": "${workspaceFolder}/Bokuract/Bokuract/bin/Debug/net5.0/linux-x64/Bokuract.dll",
            "args": [ "/lab/romhacking/psp/Boku-no-Natsuyasumi/GameData" ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": ".NET Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}